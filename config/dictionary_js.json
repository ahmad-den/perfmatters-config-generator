{
  "universal": {
    "js_exclusions": [
      "gtm",
      "gtag",
      "googletag",
      "dataLayer"
    ]
  },
  "plugins": {
    "example-plugin": {
      "js_exclusions": [
        "example-script"
      ]
    },
    "dummy-plugin": {
      "js_exclusions": [
        "dummy-script"
      ]
    }
  },
  "themes": {
    "example-theme": {
      "js_exclusions": [
        "example-theme-script"
      ]
    },
    "dummy-theme": {
      "js_exclusions": [
        "dummy-theme-script"
      ]
    }
  }
}